{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/privacy", "dest": "/privacy-policy.html" },
    { "src": "/pp", "dest": "/privacy-policy.html" },
    { "src": "/terms", "dest": "/terms-and-conditions.html" },
    { "src": "/tos", "dest": "/terms-and-conditions.html" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
